  "http://purl.org/dc/terms/creator" : [ {
    "@id" : "_:genid1"
  } ],
  "http://purl.org/dc/terms/description" : [ {
    "@language" : "en",
    "@value" : "This ontology contains a preliminary list of properties that might prove useful to describe heritage buildings. Besides two taxonomies of general built heritage properties, extending resp. contax:property_OP and contax:property_DP, it also contains terminology for the more specific patterns related to construction assessments (extending the more generic Construction Properties (CP) taxonomy). The properties are inspired by (1) the databases of Monumentenwacht Vlaanderen (MAKSBO and IMACS), (2) the online built heritage inventory of Onroerend Erfgoed Vlaanderen and (3) the results of interviews with a large variety of Flemish built heritage stakeholders."
  } ],
  "http://purl.org/dc/terms/issued" : [ {
    "@type" : "http://www.w3.org/2001/XMLSchema#date",
    "@value" : "2020-12-19"
  } ],
  "http://purl.org/dc/terms/title" : [ {
    "@language" : "en",
    "@value" : "Built Heritage Properties (BHP) taxonomy"
  } ],
  "http://purl.org/vocab/vann/preferredNamespacePrefix" : [ {
    "@value" : "bhp"
  } ],
  "http://purl.org/vocab/vann/preferredNamespaceUri" : [ {
    "@value" : "https://w3id.org/con-tax/properties/bhp#"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "- Version 0.0.1: initial version"
  } ],
  "http://www.w3.org/2002/07/owl#versionInfo" : [ {
    "@value" : "0.0.1"
  } ]
